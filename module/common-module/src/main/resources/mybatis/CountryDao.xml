<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE  mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.rpframework.module.common.dao.ICountryDao">

	<resultMap type="com.rpframework.module.common.domain.Country" id="mCountry" autoMapping="true" />

	<select id="queryAll" resultMap="mCountry">
		select * from country where 1=1
	</select>
	
	<update id="update" parameterType="com.rpframework.module.common.domain.Country" />
	<insert id="insert" parameterType="com.rpframework.module.common.domain.Country" useGeneratedKeys="true" keyProperty="code"/>
	<delete id="delete" parameterType="com.rpframework.module.common.domain.Country" />
	<select id="select" parameterType="String" resultMap="mCountry">
		select * from country where code=#{code}
	</select> 
</mapper>
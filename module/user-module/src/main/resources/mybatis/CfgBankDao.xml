<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE  mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.rpframework.module.user.dao.ICfgBankDao">

	<resultMap type="com.rpframework.module.user.domain.CfgBank" id="mCfgBank" autoMapping="true">
		<id property="id" column="id" />
	</resultMap>

	<select id="doPager" parameterType="map" resultMap="mCfgBank">
		select * from cfg_bank where 1=1
		
		<if test="page.searchMap.state != null">
			and state = #{page.searchMap.state} 
		</if>
		
		order by id
	</select>
	
	<update id="update" parameterType="com.rpframework.module.user.domain.CfgBank" />
	<insert id="insert" parameterType="com.rpframework.module.user.domain.CfgBank" useGeneratedKeys="true" keyProperty="id"/>
	<delete id="delete" parameterType="com.rpframework.module.user.domain.CfgBank" />
	<select id="select" parameterType="Integer" resultMap="mCfgBank">
		select * from cfg_bank where id=#{id}
	</select> 
</mapper>
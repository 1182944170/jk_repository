<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE  mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.rpframework.module.user.dao.IUserMoneyLogDao">

	<resultMap type="com.rpframework.module.user.domain.UserMoneyLog" id="mUserMoneyLog" autoMapping="true">
		<id property="id" column="id" />
	</resultMap>
	
	<select id="doPager" parameterType="map" resultMap="mUserMoneyLog">
		select * from u_money_log where 1=1
		<if test="page.searchMap.userId != null">
			and userId = #{page.searchMap.userId} 
		</if>
		 order by id desc
	</select>

	<update id="update" parameterType="com.rpframework.module.user.domain.UserMoneyLog" />
	<insert id="insert" parameterType="com.rpframework.module.user.domain.UserMoneyLog" useGeneratedKeys="true" keyProperty="id"/>
	<delete id="delete" parameterType="com.rpframework.module.user.domain.UserMoneyLog" />
	<select id="select" parameterType="Integer" resultMap="mUserMoneyLog">
		select * from u_money_log where id=#{id}
	</select> 
</mapper>
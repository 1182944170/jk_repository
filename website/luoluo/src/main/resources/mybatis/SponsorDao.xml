<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE  mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.rpframework.website.luoluo.dao.ISponsorDao">

	<resultMap type="com.rpframework.website.luoluo.domain.Sponsor" id="Sponsor" autoMapping="true">
		<id property="id" column="id" />
	</resultMap>		

	<select id="doPager" resultMap="Sponsor">
		select * from sponsor where 1=1
		<if test = "page.searchMap.teyeid!= null">
			and type = #{page.searchMap.teyeid}
		</if>
	</select>

	<update id="update" parameterType="com.rpframework.website.luoluo.domain.Sponsor" />
	<insert id="insert" parameterType="com.rpframework.website.luoluo.domain.Sponsor" useGeneratedKeys="true" keyProperty="id"/>
	<delete id="delete" parameterType="com.rpframework.website.luoluo.domain.Sponsor" />
	<select id="select" parameterType="Integer" resultMap="Sponsor">
		select * from sponsor where id=#{id} 
	</select>
</mapper>
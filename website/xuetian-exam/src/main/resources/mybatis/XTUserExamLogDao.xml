<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE  mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.rpframework.website.xtexam.dao.IXTUserExamLogDao">
	<resultMap type="com.rpframework.website.xtexam.domain.XTUserExamLog" id="mXTUserExamLog" autoMapping="true">
		<id property="id" column="id"/>
		<association property="user" column="userId" select="com.rpframework.website.xtexam.dao.IXTUserDao.select" />
		<association property="examClassify" column="examClassifyId" select="com.rpframework.website.xtexam.dao.IExamSubChapterClassifyDao.select" />
		<collection property="examSubjectLogs" column="id" select="com.rpframework.website.xtexam.dao.IXTUserExamSubjectLogDao.getListByExamLogId" />
	</resultMap>
	
	<update id="update" parameterType="com.rpframework.website.xtexam.domain.XTUserExamLog" />
	<insert id="insert" parameterType="com.rpframework.website.xtexam.domain.XTUserExamLog" useGeneratedKeys="true" keyProperty="id"/>
	<delete id="delete" parameterType="com.rpframework.website.xtexam.domain.XTUserExamLog" />
	<select id="select" parameterType="Integer" resultMap="mXTUserExamLog">
		select * from xt_user_exam_log where id=#{id}
	</select>
	
	<select id="findExamLogByUserId" parameterType="Integer" resultMap="mXTUserExamLog">
		select * from xt_user_exam_log where userId=#{userId}
	</select>
</mapper>
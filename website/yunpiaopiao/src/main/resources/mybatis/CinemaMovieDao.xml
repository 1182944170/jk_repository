<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE  mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.rpframework.website.yunpiaopiao.dao.ICinemaMovieDao">
	<resultMap type="com.rpframework.website.yunpiaopiao.domian.CinemaMovie" id="mCinemaMovie" autoMapping="true">
		<id property="id" column="id"/>
		<association property="cinema" column="cinemaId" select="com.rpframework.website.yunpiaopiao.dao.ICinemaDao.select"/>
	</resultMap>
	
	<select id="doPager" resultMap="mCinemaMovie">
		select * from cinema_movie where 1=1
	</select>
	
	
	<select id="getCinemaMovieByCinema" parameterType="Integer" resultMap="mCinemaMovie">
		select * from cinema_movie where 1=1 and cinemaId=#{cinemaId}
	</select>
	
	<select id="getCinemaMovieByMovieId" parameterType="Integer" resultMap="mCinemaMovie">
		select * from cinema_movie where 1=1 and movieId=#{movieId}
	</select>
	
	<delete id="deleteByCinemaId" parameterType="Integer">
		delete from cinema_movie where cinemaId=#{cinemaId}
	</delete>
	
	<delete id="deleteByMovieId" parameterType="Integer">
		delete from cinema_movie where movieId=#{movieId}
	</delete>
	
	<update id="update" parameterType="com.rpframework.website.yunpiaopiao.domian.CinemaMovie" />
	<insert id="insert" parameterType="com.rpframework.website.yunpiaopiao.domian.CinemaMovie" useGeneratedKeys="true" keyProperty="id"/>
	<delete id="delete" parameterType="com.rpframework.website.yunpiaopiao.domian.CinemaMovie" />
	<select id="select" parameterType="Integer" resultMap="mCinemaMovie">
		select * from cinema_movie where id=#{id}
	</select>
</mapper>